{
    "name": "@elizaos/plugin-bootstrap",
    "version": "0.1.9-alpha.1",
    "type": "module",
    "main": "dist/index.js",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        "./package.json": "./package.json",
        ".": {
            "import": {
                "@elizaos/source": "./src/index.ts",
                "types": "./dist/index.d.ts",
                "default": "./dist/index.js"
            }
        }
    },
    "files": [
        "dist"
    ],
    "dependencies": {
        "@elizaos/core": "workspace:*",
        "tsup": "8.3.5"
    },
    "scripts": {
        "build": "NODE_DEBUG='module,child_process,net,stream,timer' DEBUG='tsup:*,pnpm:*' TSUP_LOG_LEVEL=verbose tsup src/index.ts --format cjs,esm --dts --clean --onSuccess 'echo Build completed successfully' --onError 'echo Build failed with error code $?'",
        "dev": "NODE_DEBUG='module,child_process,net,stream,timer' DEBUG='tsup:*,pnpm:*' TSUP_LOG_LEVEL=verbose tsup src/index.ts --format cjs,esm --dts --watch"
    },
    "peerDependencies": {
        "whatwg-url": "7.1.0"
    },
    "devDependencies": {
        "cross-env": "^7.0.3"
    }
}